/* pages/projectDetail/projectDetail.wxss */
 @import "../../libs/wxParse/wxParse.wxss";
.page{
  overflow: hidden;
  padding: 0 30rpx;
  /* background: #EDEDED; */
}
.head{
  border-bottom: 1rpx solid #A5D8D5;
  display: flex;
  align-items: center;
  padding: 40rpx 0 28rpx;
}
.shu{
  width:4rpx;
  height: 28rpx;
  background: #A5D8D5;
  margin-right: 20rpx;
}
.title{
  font-size: 30rpx;
  color: #333;
}
.bigimg{
  width:690rpx;
  height:500rpx;
  border-radius:12px;
  overflow: hidden;
  margin-top: 28rpx;
}
.bigimg image{
  width: 100%;
  height: 100%;
}
.yuan{
  width: 12rpx;
  height: 12rpx;
  border-radius: 100%;
  background: #04A2AF;
  margin-right: 12rpx;
}
.descriptionTitle{
  font-size: 28rpx;
  color: #333;
}
.descriptionbBox{
  display: flex;
  align-items: center;
  padding: 26rpx 0;
  border-bottom: 1px solid #A5D8D5;
}
.description{
  border-top: 4rpx solid #EDEDED;
  margin-top: 30rpx;
  padding: 0 30rpx;
}
.descriptionContent{
  font-size: 28rpx;
  color: #707070;
  padding-top: 30rpx;
}